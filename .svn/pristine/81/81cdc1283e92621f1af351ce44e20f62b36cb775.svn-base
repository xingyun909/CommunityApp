<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <title>智慧社区</title>
    <link rel="stylesheet" type="text/css" href="css/index.css" />
    <link rel="stylesheet" type="text/css" href="css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="css/swipe.css">
     <link rel="stylesheet" href="css/bottom.css">
    <script type="text/javascript" src="lib/jquery.min.js"></script>
    <script type="text/javascript" src="lib/jquery-ui.min.js"></script>
    <script type="text/javascript" src="lib/jquery.simplesidebar.js"></script>
</head>

<body ng-controller="indexCtrl">
    <div>
        <!-- 头部 -->
        <div id="framecontentTop">
            <div class="header">
                智慧社区
            </div>
        </div>
        <!-- 内容 -->
        <div id="maincontent">
            <!-- 轮播 -->
            <div class="addWrap">
                <div class="swipe" id="mySwipe">
                    <div class="swipe-wrap">
                        <div>
                            <a href="javascript:;"><img class="img-responsive" src="images/index/4.jpg" /></a>
                        </div>
                        <div>
                            <a href="javascript:;"><img class="img-responsive" src="images/index/5.jpg" /></a>
                        </div>
                        <div>
                            <a href="javascript:;"><img class="img-responsive" src="images/index/6.jpg" /></a>
                        </div>
                    </div>
                </div>
                <ul id="position">
                    <li class="cur"></li>
                    <li class=""></li>
                    <li class=""></li>
                </ul>
            </div>
            <!-- 导航 -->
            <div class="nav .containerx">
                <div class="col-xs-12">
                    <a  ng-click="check('noticeA')" class="col-xs-4">
                        <div>
                            <img src="images/index/icon_11.png" alt="社区公告" />
                            <p>社区公告</p>
                        </div>
                    </a>
                    <a ui-sref="baoxiu_apply" class="col-xs-4">
                        <div>
                            <img src="images/index/icon_17.png" alt="物业报修" />
                            <p>物业报修</p>
                        </div>
                    </a>
                    <a ui-sref="wait" class="col-xs-4">
                        <div>
                            <img src="images/index/icon_11.png" alt="家政服务" />
                            <p>家政服务</p>
                        </div>
                    </a>
                    <a ui-sref="wait" class="col-xs-4">
                        <div>
                            <img src="images/index/icon_13.png" alt="智能家居" />
                            <p>智能家居</p>
                        </div>
                    </a>
                    <a ui-sref="wait" class="col-xs-4">
                        <div>
                            <img src="images/index/icon_13.png" alt="家教中心" />
                            <p>家教中心</p>
                        </div>
                    </a>
                    <a href="" class="col-xs-4">
                        <div>
                            <img src="images/index/icon_13.png" alt="更多" />
                            <p>更多</p>
                        </div>
                    </a>
                    <!--<a href=""class="col-xs-3"><img src="images/icon_13.png" alt="" /><p>物业报修</p></a>
                    <a href=""class="col-xs-3"><img src="images/icon_17.png" alt="" /><p>物业报修</p></a>-->
                </div>
            </div>
            <!-- 广告 -->
            <div>
                <div class="adv">
                    <img src="images/index/adv.png" />
                </div>
            </div>
        </div>
        <!-- 底部 -->
         <div id="framecontentBottom">
        <div class="footer ">
            <div class="col-xs-12 ">
                <a ui-sref="index" class="col-xs-4"><img src="images/shop_main/icon_home.png" alt="" />
                    <p class="p">主页</p>
                </a>
                <a ui-sref="shop_main" class="col-xs-4"><img src="images/shop_main/icon_around.png" alt="" />
                    <p>周边</p>
                </a>
                <a ui-sref="personal" class="col-xs-4"><img src="images/shop_main/icon_me.png" alt="" />
                    <p>我的</p>
                </a>
            </div>
        </div>
    </div>
</body>
<script src="js/swipe.js"></script>
<script type="text/javascript">
var bullets = document.getElementById('position').getElementsByTagName('li');
var banner = Swipe(document.getElementById('mySwipe'), {
    auto: 2000,
    continuous: true,
    disableScroll: false,
    callback: function(pos) {
        var i = bullets.length;
        while (i--) {
            bullets[i].className = ' ';
        }
        bullets[pos].className = 'cur';
    }
});
</script>

</html>
