@charset "UTF-8";
body,
ul,
li,
p,
form,
img,
div,
input {
    margin: 0;
    padding: 0;
    font-family: Microsoft YaHei, sans-serif;
}

.maintop {
    height: 120px;
    background: #f4f4f4;
    border-bottom: 1px solid #a0a0a0;
    border-top: 1px solid #a0a0a0;
}

#pimg {
    height: 100px;
    width: 35%;
    float: left;
}

#pimg img {
    height: 86px;
    border-radius: 43px;
    margin: 17px 25px;
}

#ptext {
    padding: 17px;
    font-size: 18px;
    float: left;
}

#pinto {
    text-decoration: none;
    position: absolute;
    right: -6px;
}

#myinto {
    float: left;
}

.list {
    border-top: 10px solid #f4f4f4;
}

.list li {
    margin: 0px 3px;
    list-style: none;
    border-bottom: 1px solid #a0a0a0;
    height: 48px;
    line-height: 48px;
    padding-left: 30px;
    font-size: 15px;
}

.list img {
    position: relative;
    top: -1px;
    left: -10px;
}

.list a {
    display: block;
    width: 100%;
    color: black;
    text-decoration: none;
}

#fenge {
    width: 100%;
    height: 143px;
}
