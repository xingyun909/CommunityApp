<link rel="stylesheet" href="css/fill_personal.css">
<script src="lib/jquery.form.js"></script>
<div ng-controller="fill_personalCtrl">
    <div class="maintop">
        <div id="pimg">
            <label for="picture"><img src="images/baoxiu/addpic.jpg" /></label>
            <input type="file" id="picture" name="picture" style="display:none">
        </div>
        <div id="ptext">
            头像修改
        </div>
    </div>
    <div class="mainbottom">
        <form id="formid" method="" ng-controller="allCtrl">
            <ul class="list">
                <li class="a">业主姓名
                    <input name="name" class="fillin" type="text" value={{name}} />
                    <img src="images/personal/fill.png"> </li>
                <li class="a">性别
                    <input name="sex" class="fillin" type="text" value={{sex}} />
                    <img src="images/personal/fill.png"> </li>
                <li class="a xiaoqu">小区
                    <input name="xiaoqu" class="fillin xqu" type="text" value={{xiaoqu}} />
                    <img src="images/personal/fill.png"> </li>
                <li class="a addre">详细住址
                    <input class="fillin adr" type="text" value={{address}} />
                    <img src="images/personal/fill.png"> </li>
                <li class="a">联系电话
                    <input name="phone" class="fillin" type="text" value={{phone}} />
                    <img src="images/personal/fill.png"> </li>
                <li class="a">身份证号
                    <input name="id" class="fillin" type="text" value={{IDCardNumber}} />
                    <img src="images/personal/fill.png"> </li>
            </ul>
            <div class="alert alqu">
                <span id="altop">请选择家乡</span>
                <ul>
                    <li class="bian">杭州 万达小区</li>
                    <li>哈尔滨 合家欢小区</li>
                </ul>
                <button class="albtn yes">确定</button>
                <button class="albtn no">取消</button>
            </div>
            <div class="alert aladdre">
                <span id="altop">请填写详细地址</span>
                <input name="addressDong" class="ad1" type="text" />栋
                <input name="addressDanyuan" class="ad2" type="text" />单元
                <input name="addressShi" class="ad3" type="text" />室
                <button class="albtn yes">确定</button>
                <button class="albtn no">取消</button>
            </div>
    </div>
    <input type="submit" id="btn" class="btn" value="确定" ng-click="send()" />
    </form>
</div>
