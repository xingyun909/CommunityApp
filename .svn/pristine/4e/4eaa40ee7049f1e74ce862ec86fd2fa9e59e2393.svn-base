
/*顶部*/
#framecontentTop {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 50px;
    /*Height of top frame div*/
    
    overflow: hidden;
    /*Disable scrollbars. Set to "scroll" to enable*/
}
.header {
    /*background-image: url(../images/header.png);*/
    
    background-color: #2b8de5;
    background-size: contain;
    color: #FFFFFF;
    font-family: 微软雅黑;
    font-size: 20px;
    height: 50px;
    vertical-align: center;
    text-align: center;
    line-height: 50px;
}
.header #img1 {
    width: 15px;
    height: 20px;
    left: 20px;
    top: 15px;
    position: absolute;
}
.header #imgr {
    width: 15px;
    height: 20px;
    right: 20px;
    top: 15px;
    position: absolute;
}
.header .title {
    margin: auto 10px;
}